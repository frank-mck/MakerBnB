<body>
<!-- NavBar -->
<nav class="navbar ">
   <div class="container">
     <a class="navbar-brand" href="/"> 
       <img src="https://i.imgur.com/IdpwQZw.png" alt="MakersBnB" width="30" height="24"> 
     </a>
     <span class="navbar-text">
      MakersBnB
    </span>
     <button class ='btn nav-btn' type ='submit' name ='log_out' value ='Log out'>Log out</button>
   </div>
 </nav>
<div class="new-list">
  <div class="modal-body row">

      <h1 class="welcome text-new"> List my spaces </h1>

    <div class="container">
      <div class="float-md-center">
        <div class="jumbotron">

            <form new-display action="/listing" method="post">
              <input type="text" name="name" placeholder ='Name of property...'/>
              <input type="text" name="description" placeholder="Description..." required/>
              <input type="text" name="price" placeholder="Price..." />
              <input class ='date btn-list' type="date" name="start_date" />
              <input class ='date btn-list' type="date" name="finish_date" />
              <input class ='date btn-list' type="submit" value="List my spaces" />
            </form>

        </div>
      </div>
    </div>
</div>





</div>



<!-- JS for Bootstrap -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous">
